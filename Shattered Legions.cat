        <selectionEntry type="upgrade" import="true" name="XVIII: Salamanders (MT | Minor/Flaw)" hidden="true" id="3063-a59a-ed33-02b8" publicationId="d882-d2a-5da1-92c4" page="147" sortIndex="16">
          <rules>
            <rule name="MT|SAL Steadfast Bulwark (Minor)" id="2636-f334-c602-ed42" hidden="false" publicationId="d882-d2a-5da1-92c4" page="147">
              <description>During a turn in which this unit is successfully Charged, all models in this unit gain the Feel No Pain (5+) special rule until the end of the turn.</description>
            </rule>
            <rule name="MT|SAL Prudent Fighters (Flaw)" id="69f2-175f-c745-8f31" hidden="false" publicationId="d882-d2a-5da1-92c4" page="147">
